{% extends 'layouts/base.njk' %}

{% import 'components/include/CommonButton/common-button.njk' as component %}

{% block contents %}
  <p>Top Page Contents</p>
  <img src="/assets/image/vite.svg" alt="vite" width="100" height="100" decoding="async"/>
  <img src="/assets/image/vituum.svg" alt="vituum" width="100" height="100" decoding="async"/>
  {{ component.Button({label:'BUTTON'}) }}
  <p>URL: {{ url }}</p>
  <p>
    <img src="/assets/image/a.webp" alt="a" width="300" height="300"/>
  </p>
  <p style="width: 1.5rem;">
    <svg viewBox="0 0 24 24">
      <use href="/assets/svg/icons.svg#arrow-right"/>
    </svg>
    <svg viewBox="0 0 24 24">
      <use href="/assets/svg/icons.svg#bolt"/>
    </svg>
    <svg viewBox="0 0 24 24">
      <use href="/assets/svg/icons.svg#moon"/>
    </svg>
  </p>
{% endblock %}