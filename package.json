{
  "name": "vituum-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:convert": "node 'scripts/convertImages.js'",
    "dev:all": "npm-run-all -p dev:convert dev",
    "build": "tsc && vite build && npm run build:format",
    "build:format": "prettier --write 'dist/**/*.html' --ignore-path .prettierignore",
    "preview": "vite preview",
    "svg": "node 'scripts/svgSprite.js'"
  },
  "devDependencies": {
    "@markuplint/nunjucks-parser": "^4.6.2",
    "@types/fs-extra": "^11.0.4",
    "@typescript-eslint/eslint-plugin": "^7.10.0",
    "@typescript-eslint/parser": "^7.10.0",
    "@vituum/vite-plugin-nunjucks": "^1.1.0",
    "chokidar": "^3.6.0",
    "eslint": "^8.56.0",
    "fs-extra": "^11.2.0",
    "glob": "^10.4.2",
    "markuplint": "^4.8.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "sass": "^1.77.2",
    "sharp": "^0.33.4",
    "stylelint": "^16.5.0",
    "stylelint-config-recess-order": "^5.0.1",
    "stylelint-config-standard-scss": "^13.1.0",
    "svg-sprite": "^2.0.4",
    "typescript": "^5.4.5",
    "vite": "^5.2.11",
    "vite-tsconfig-paths": "^4.3.2",
    "vituum": "^1.1.0"
  }
}
